{
  "name": "slate",
  "version": "0.0.0",
  "main": "functions.php",
  "author": "Nate Weller <hello@nateweller.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.6.3",
    "@wordpress/scripts": "^12.3.0",
    "browser-sync": "^2.26.13",
    "concurrently": "^5.3.0",
    "dir-archiver": "^1.1.2",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-webp": "^6.0.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.5",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.1.0",
    "readline-sync": "^1.4.10",
    "uglify-js": "^3.12.1",
    "webp-converter": "^2.3.1"
  },
  "scripts": {
    "test": "node scripts.js",
    "watch": "concurrently --kill-others \"yarn watch:styles\" \"yarn watch:scripts\" \"yarn browser-sync start -c bs-config.js\"",
    "watch:styles": "nodemon -e scss --exec \"yarn styles\"",
    "watch:scripts": "nodemon -e js --exec \"yarn scripts\" --ignore '*.min.js' --ignore '*.map.js'",
    "styles": "node scripts.js styles",
    "scripts": "node scripts.js scripts",
    "images": "node scripts.js images",
    "compile": "node scripts.js compile",
    "lint:scss": "wp-scripts lint-style 'styles/**/*.scss'",
    "lint:js": "wp-scripts lint-js 'js/*.js'",
    "bundle": "dir-archiver --src . --dest ../_slate.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json yarn.lock package.json .travis.yml phpcs.xml.dist sass style.css.map",
    "slate": "node scripts.js slate"
  }
}
